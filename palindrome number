class solution {
  public boolean isPalindrome(int x){
    if(x ==0) {
      return True;
    }
    
    if (x < 0 || x % 10 == 0) {
      return False
    }
    
    int reversed_int = 0;
    while (x > reversed_int) {
      int pop = x % 10;
      x /= 10;
      
      reversed_int = (reversed * 10) + pop;
    }
    
    if ( x==reversed_int || x ==reversed_int / 10) {
      return True;
    }
    else 
    {return False }
  }
}
