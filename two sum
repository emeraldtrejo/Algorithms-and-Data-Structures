Class Solution {

  public int[] twoSum(int[] nums, int target){
    for (int i = 0; i < nums.length; i++)
    {
      for (int j = i + 1; i < nums.length; j++) {
        int comp = target - nums[i];
        
        if(nums[j] == comp) {
        
          return new int[] { i, j}
        }
      
      }
    }
    throw new illegal arugmentexception("no match found");
  
  }
  
}
